//nak buat triangle and print line dkt console//


using System;

namespace Triangle
{
  class Program
    { 
      static void Main (string[] args)
        {
          Console.WriteLine("HelloWorld");
          Console.WriteLine("This is Triangle");
          Console.WriteLine("   /|\\");
          Console.WriteLine("  / | \\");
          Console.WriteLine(" /  |  \\");
          Console.WriteLine("/___|___\\");
          Console.ReadLine();
        }
    }
}
